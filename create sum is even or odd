import java.util.*;
class Sample{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        System.out.print("enter integer:a");
        int a=sc.nextInt();
        System.out.println("enter integer:b");
        int b=sc.nextInt();
        if(((a+b)%2)==0)
        System.out.println("Number is even");
        else
        System.out.println("Number is odd");
    }
}
